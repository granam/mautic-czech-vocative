{
  "name": "friendsofmautic/vocative",
  "license": "MIT",
  "type": "mautic-bundle",
  "description": "Modifier for first names to convert their base form to vocative",
  "autoload": {
    "psr-4": {
      "MauticPlugin\\": "FOM/"
    }
  },
  "require": {
    "php": ">=7.0",
    "ext-iconv": "*"
  },
  "require-dev": {
    "granam/czech-vocative": "~2.0",
    "friendsofmautic/bundle-skeleton": "^2.11",
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mautic/mautic.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "pre-autoload-dump": "MauticPlugin\\MauticVocativeBundle\\MauticVocativeBundle::includeLibraryToBundle"
  }
}
