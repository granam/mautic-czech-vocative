{
  "name": "granam/mautic-vocative-bundle",
  "license": "MIT",
  "description": "Modifier for first names to convert their base form to vocative",
  "type": "mautic-plugin",
  "extra": {
    "install-directory-name": "GranamVocativeBundle"
  },
  "autoload": {
    "psr-4": {
      "MauticPlugin\\GranamVocativeBundle\\": "."
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MauticPlugin\\GranamVocativeBundle\\Tests\\": "Tests"
    }
  },
  "require": {
    "php": ">=7.3",
    "granam/czech-vocative": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "mautic/core-lib": "^4.4",
    "mockery/mockery": "~1.0",
    "rector/rector": "^0.14.2"
  },
  "scripts": {
    "pre-autoload-dump": "MauticPlugin\\GranamVocativeBundle\\GranamVocativeBundle::includeLibraryToBundle"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
